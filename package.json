  },
  "homepage": "https://github.com/bamboo-connection/map-side-bar#readme",
  "engines": {
    "node": ">=8.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jsdom": "^11.6.2",
    "nodemon": "^1.17.2",
    "sinon": "^4.4.8",
    "style-loader": "^0.20.3",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "axios": "^0.18.0",
